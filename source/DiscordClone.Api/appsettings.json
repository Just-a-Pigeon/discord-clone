{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "apiKey": ""
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ],
    "Properties": {
      "ApplicationName": "discord-clone"
    }
  },
  "Postgres": {
    "Host": "localhost",
    "Database": "discordclone",
    "Port": "5432",
    "Username": "postgres",
    "Password": "mysecretpassword"
  },
  "JWTConfiguration": {
    "Issuer": "SignalRChat Test",
    "Audience": "https://localhost:5001",
    "SigningKey": "my long enough key for authentication",
    "TokenExpirationDays": 7
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SwaggerServerHost": "localhost:5000",
  "SwaggerServerUrl": "http://localhost:5000"
}
